{
    "name": "laravel-bridge/container",
    "description": "The PSR-11 container bridges",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "container", "psr11"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "illuminate/container": "^5.6 | ^6 | ^7",
        "laravel-bridge/scratch": "@stable",
        "php-di/php-di": "^6.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^7 | ^8",
        "pimple/pimple": "^3.0",
        "psy/psysh": "^0.9.12",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelBridge\\Container\\": "src/Container"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
