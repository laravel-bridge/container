{
    "name": "laravel-bridge/container",
    "description": "The PSR-11 container bridges",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "container", "psr11"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^7 | ^8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelBridge\\Container\\": "src/Container"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
